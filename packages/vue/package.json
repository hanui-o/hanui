    "design-system",
    "government",
    "public",
    "accessibility",
    "a11y",
    "hanui"
  ],
  "homepage": "https://hanui.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/hanui-o/hanui.git",
    "directory": "packages/vue"
  },
  "bugs": {
    "url": "https://github.com/hanui-o/hanui/issues"
  },
  "author": "hanui-o",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
